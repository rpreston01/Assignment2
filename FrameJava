    private void tFServerAddressFocusLost(java.awt.event.FocusEvent evt) {
        GetAndSet.setWebsite(tFServerAddress.getText(),tFNumberPages.getText());
    }

    private void tFNumberPagesFocusLost(java.awt.event.FocusEvent evt) {
        GetAndSet.setWebsite(tFServerAddress.getText(),tFNumberPages.getText());
    }

    private void comboBoxFocusGained(java.awt.event.FocusEvent evt) {
        int iCounter = 0;
        String comboBoxContents; 
        
        for(int i = 0; i < GetAndSet.updateArray() ;i++){
            comboBoxContents = GetAndSet.getArray()[iCounter];
            comboBox.addItem(comboBoxContents);
            iCounter++;
        }
        
        
    }

